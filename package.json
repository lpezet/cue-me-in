{
  "name": "@lpezet/cue-me-in",
  "version": "1.0.0",
  "description": "Library to help monitor processes or web pages.",
  "main": "lib/index",
  "types": "lib/index.d.ts",
  "scripts": {
    "dist": "npm run clean && npm run tsc -- --build tsconfig.publish.json",
    "tsc": "tsc",
    "clean": "rimraf lib",
    "lint": "eslint --ext .ts,.tsx .",
    "mocha": "nyc mocha --recursive src/test/**/*",
    "mocha-single": "nyc mocha --require ts-node/register --require source-map-support/register --require src/test/helpers/mocha-bootstrap.js --timeout=10000",
    "test-integration": "ts-node",
    "test": "npm run lint -- --quiet && npm run mocha",
    "test:client-integration": "./scripts/client-integration-tests/run.sh",
    "test:extensions-emulator": "./scripts/extensions-emulator-tests/run.sh"
  },
  "files": [
    "lib",
    "templates",
    "standalone"
  ],
  "preferGlobal": true,
  "engines": {
    "node": ">= 8.0.0"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".js",
      ".ts"
    ],
    "exclude": [
      "src/test/**/*"
    ]
  },
  "author": "",
  "license": "ISC",
